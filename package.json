{
  "name": "elguanteweb-react",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "start": "concurrently \"npm run dev\" \"npm run backend\"",
    "backend": "node backend/server.js",
    "lint": "eslint src --ext js,jsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint src --ext js,jsx --fix",
    "format": "prettier --write \"src/**/*.{js,jsx,json,css,md}\"",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "build:prod": "ng build emsmart --configuration=production && yarn sentry:sourcemaps",
    "build:preprod": "ng build emsmart --configuration=preprod && yarn sentry:sourcemaps",
    "build:apm": "ng build emsmart --configuration=apm && yarn sentry:sourcemaps",
    "build:stage": "ng build emsmart --configuration=stage && yarn sentry:sourcemaps",
    "build:dev": "ng build emsmart --configuration=development && yarn build-storybook && yarn sentry:sourcemaps",
    "build:qa": "ng build emsmart --configuration=qa && yarn sentry:sourcemaps"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "csv-parse": "^6.1.0",
    "csv-parser": "^3.2.0",
    "dotenv": "^17.2.2",
    "express": "^5.1.0",
    "express-rate-limit": "^7.1.5",
    "firebase": "^12.2.1",
    "firebase-admin": "^13.5.0",
    "mercadopago": "^2.9.0",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.23.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.1.4",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.5.1",
    "@types/react": "^18.2.37",
    "@types/react-dom": "^18.2.15",
    "@vitejs/plugin-react": "^4.2.0",
    "@vitest/coverage-v8": "^0.34.6",
    "@vitest/ui": "^0.34.6",
    "concurrently": "^9.2.1",
    "eslint": "^8.53.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.4",
    "jsdom": "^22.1.0",
    "prettier": "^3.0.3",
    "vite": "^5.3.3",
    "vitest": "^0.34.6"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  }
}
